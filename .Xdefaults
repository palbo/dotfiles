
Xft.dpi:96
Xft.hinting:true
Xft.hintstyle:hintfull

!---------------------

XTerm*font: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
XTerm*boldFont: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
XTerm*bellIsUrgent: true
XTerm*geometry: 115x50
XTerm*highlightSelection: true
XTerm*eightBitInput: false
XTerm*saveLines: 5000
XTerm*termName: xterm-256color
!XTerm*selectToClipboard: true
XTerm*VT100.translations: #override \
  <BtnUp>:select-end(PRIMARY, CUT_BUFFER0, CLIPBOARD)

XTerm*color4: DodgerBlue1
XTerm*color12: DodgerBlue2

!---------------------

URxvt*font: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
URxvt*italicFont: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
!URxvt*font: -*-proggytiny-*-*-*-*-*-*-*-*-*-*-*-*
!URxvt*italicFont: -*-proggytiny-*-*-*-*-*-*-*-*-*-*-*-*
!URxvt*boldFont: -*-proggytiny-*-*-*-*-*-*-*-*-*-*-*-*

URxvt*geometry: 115x50
URxvt*externalBorder: 0
URxvt*internalBorder: 0
URxvt*scrollBar: false
!URxvt*transparent: true
!URxvt*shading: 80
!URxvt*tintColor: #ffffff
!URxvt*depth: 32
!URxvt*background: rgba:0000/0000/0000/0000
!URxvt*iconFile: /usr/share/icons/gnome/32x32/apps/terminal.png
URxvt*iconFile: /usr/share/pixmaps/rxvt-unicode.png

URxvt*tripleclickwords: true
URxvt*saveLines: 4096
URxvt*iso14755_52: false
URxvt*urgentOnBell: true

URxvt*perl-ext: selection,selection-popup,matcher
URxvt*perl-ext-common: 
URxvt*urlLauncher: /usr/bin/opera
URxvt*keysym.C-M-w: perl:matcher
URxvt*keysym.C-M-S-p: perl:searchable-scrollback
URxvt*matcher.button: 3

